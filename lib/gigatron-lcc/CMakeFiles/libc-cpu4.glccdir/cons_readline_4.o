#VCPUv4

# ======== ('CODE', 'echo', code0)
def code0():
	label('echo');
	_PROLOGUE(12,6,0x80); # save=R7
	LDW(R8);STW(R7);
	_SP(-1+12);STW(R23);
	LDI(63);POKE(R23);
	LDW(R7);XORI(10);_BEQ('.18');
	LDW(R7);_CMPIS(32);_BLT('.16');
	LDI(131);SUBW(R7);_BLE('.16');
	label('.18');
	_SP(-1+12);STW(R23);
	LDW(R7);POKE(R23);
	label('.16');
	_SP(-1+12);STW(R8);
	LDI(1);STW(R9);
	LDWI('console_print');CALL(vAC);
	label('.15');
	_EPILOGUE(12,6,0x80);
# ======== ('CODE', 'console_readline', code1)
def code1():
	label('console_readline');
	_PROLOGUE(16,4,0xf8); # save=R3-7
	LDW(R8);STW(R7);
	LDW(R9);STW(R6);
	LDW(R7);STW(R5);
	LDW(R6);ADDW(R7);SUBI(-v(-3));STW(R4);
	LDI(0);STW(R3);
	_BRA('.23');
	label('.20');
	LDWI('console_waitkey');CALL(vAC);
	STW(R3);
	XORI(8);_BEQ('.27');
	LDW(R3);XORI(253);_BEQ('.27');
	LDW(R3);XORI(127);_BNE('.24');
	label('.27');
	LDW(R5);_CMPWU(R7);_BLE('.25');
	LDWI('.30');STW(R8);
	LDI(1);STW(R9);
	LDWI('console_print');CALL(vAC);
	LDW(R5);SUBI(-v(-1));STW(R5);
	STW(R23);
	LDI(0);POKE(R23);
	_BRA('.21');
	label('.24');
	LDW(R5);_CMPWU(R4);_BLT('.33');
	LDW(R3);XORI(10);_BNE('.31');
	label('.33');
	LDW(R3);STW(R8);
	LDWI('echo');CALL(vAC);
	LDW(R5);STW(R23);
	ADDI(1);STW(R5);
	LDW(R3);POKE(R23);
	LDI(0);POKE(R5);
	label('.31');
	label('.25');
	label('.21');
	label('.23');
	LDW(R3);XORI(10);_BNE('.20');
	LDW(R5);SUBW(R7);
	label('.19');
	_EPILOGUE(16,4,0xf8,saveAC=True);
# ======== ('DATA', '.30', code2, 0, 1)
def code2():
	label('.30');
	bytes(8,0);
# ======== (epilog)
code=[
	('CODE', 'echo', code0),
	('EXPORT', 'console_readline'),
	('CODE', 'console_readline', code1),
	('IMPORT', 'console_waitkey'),
	('IMPORT', 'console_print'),
	('DATA', '.30', code2, 0, 1) ]
module(code=code, name='../gigatron/libc/cons_readline.c', cpu=4);

