# ======== ('CODE', '_console_reset', code1)
def code1():
	label('_console_reset');
	_PROLOGUE(20,8,0xf8); # save=R3-7
	MOVW(R8,R7);
	MOVIW('videoTable',R5);
	MOVQW(48,R4);
