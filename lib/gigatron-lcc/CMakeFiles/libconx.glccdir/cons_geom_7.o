	MOVIW(0x2f00,R8);
	STW(R9);
	MOVQW(81,R10);
	CALLI('_console_clear');
	label('.16');
	MOVQW(0,R3);
	label('.18');
	MOVQW(0,R6);
	label('.22');
	LDW(R6);SUBI(2);_BLT('.28');
	LDW(R6);SUBI(10);_BLT('.26');
	label('.28');
	LDW(R5);DOKEQ(47);
	_BRA('.27');
	label('.26');
	LDW(R4);
	INCV(R4);
	DOKE(R5);
	label('.27');
	label('.23');
	INCV(R6);
	ADDSV(2,R5);
	LDW(R6);XORI(12);_BNE('.22');
	label('.19');
	INCV(R3);
	LDW(R3);XORI(10);_BNE('.18');
	label('.15');
	_EPILOGUE(20,8,0xf8);
# ======== (epilog)
code=[
	('EXPORT', 'console_info'),
	('DATA', 'console_info', code0, 20, 2),
	('EXPORT', '_console_reset'),
